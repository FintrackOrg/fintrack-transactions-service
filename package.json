{
  "name": "fintrack-transactions-service",
  "version": "1.0.0",
  "description": "",
  "main": "src/server.ts",
  "scripts": {
    "start": "nodemon --watch './**/*.ts' --exec 'ts-node' src/server.ts",
    "start:dev": "PINO_LOG_LEVEL=debug nodemon",
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf ./dist",
    "compile": "tsc",
    "generate:proto": "buf generate buf.build/darvand/fintrackapis && buf generate buf.build/googleapis/googleapis",
    "test": "jest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.245.0",
    "@aws-sdk/lib-dynamodb": "^3.245.0",
    "@grpc/grpc-js": "^1.6.7",
    "@grpc/proto-loader": "^0.6.13",
    "@protobuf-ts/runtime": "^2.8.2",
    "@protobuf-ts/runtime-rpc": "^2.8.2",
    "aws-sdk": "^2.1295.0",
    "dotenv": "^16.0.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.13.1-4",
    "@faker-js/faker": "^7.6.0",
    "@types/google-protobuf": "^3.15.6",
    "@types/jest": "^29.4.0",
    "@types/uuid": "^9.0.0",
    "grpc_tools_node_protoc_ts": "^5.3.2",
    "grpc-tools": "^1.12.3",
    "jest": "^29.4.2",
    "minimist": "^1.2.6",
    "nodemon": "^2.0.19",
    "pino": "^8.9.0",
    "pino-pretty": "^9.1.1",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.4"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "node -r tsconfig-paths/register -r ts-node/register ./src/server.ts",
    "ext": "ts, js"
  }
}
